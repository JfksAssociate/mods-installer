            </cursor>
         </Button>
         <Button layoutX="396.0" layoutY="6.0" mnemonicParsing="false" onAction="#installForge" style="-fx-background-color: rgb(207, 207, 207);" text="Install Forge">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor>
         </Button>
      </children>
   </AnchorPane>

    <Label fx:id="welcomeText" textFill="WHITE">
      <font>
         <Font size="14.0" />
      </font></Label>
   <AnchorPane prefHeight="6.0" prefWidth="600.0">
      <children>
         <TextField id="filePath" layoutX="5.0" minWidth="0.0" prefHeight="27.0" prefWidth="590.0" promptText="Enter File Path" style="-fx-cursor: TEXT; -fx-min-width: 100%;" text="\%appdata%/.minecraft/mods" />
         <Button layoutX="571.0" layoutY="1.0" mnemonicParsing="false" onAction="#openFileExplorer" prefHeight="20.0" prefWidth="15.0" style="-fx-background-color: rgb(207, 207, 207); -fx-border-radius: 0;" text="...">
