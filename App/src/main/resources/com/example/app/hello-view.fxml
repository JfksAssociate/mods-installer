<VBox alignment="CENTER" minHeight="426.0" minWidth="609.0" prefHeight="449.0" prefWidth="620.0" spacing="20.0" style="-fx-background-color: white; -fx-min-width: 620px; -fx-min-height: 440px; -fx-padding: 10px; -fx-background-image: url('https://i2.wp.com/files.123freevectors.com/wp-content/original/130957-abstract-blue-and-white-polygon-background-design.jpg?w=800&amp;q=95');" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.app.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane>
      <children>
         <WebView id="webView" layoutX="-10.0" layoutY="-14.0" prefHeight="255.0" prefWidth="629.0" />
      </children>
   </AnchorPane>
   <AnchorPane prefHeight="36.0" prefWidth="611.0">
      <children>
         <TextField id="url" prefHeight="20.0" prefWidth="600.0" promptText="Files Link" style="-fx-border-color: white; -fx-background-color: white;" text="https://www.dropbox.com/sh/7780nkp62l70ws0/AADBUryLX_enctdZyH2F6XWqa?dl=1" />
      </children>
   </AnchorPane>
   <AnchorPane prefHeight="26.0" prefWidth="600.0">
